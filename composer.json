{
    "name": "abdursoft/php",
    "description": "abdursoft php mvc framework. Just work with fun. It's supported multiple databases and a new template engine like blade. Now you can easily build your project with this framework. It has default jwt authentication system",
    "type": "project",
    "license": "MIT",
    "version": "1.0.8",
    "authors": [
        {
            "name": "Abdur Rahim",
            "email": "support@abdursoft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ABS\\Framework\\App\\": "app/",
            "ABS\\Framework\\Core\\": "core/",
            "ABS\\Framework\\System\\": "system/",
            "ABS\\Framework\\DB\\": "database/",
            "ABS\\Framework\\PLUGIN\\": "plugins/"
        },
        "files": [
            "system/helper/helper.php"
        ]
    },
    "minimum-stability": "stable",
    "require": {
        "phpmailer/phpmailer": "^6.9",
        "james-heinrich/getid3": "^1.9",
        "lcobucci/jwt": "^5.3",
        "kreait/firebase-php": "^7.13"
    }
}
